<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sk2a.hello.chann.dao.UserDao">
    <select id="getAllUsers" resultType="Map">
        SELECT * FROM user
    </select>

    <select id="getUserByUserLogin" parameterType="String" resultType="sk2a.hello.chann.domain.User">
        SELECT * FROM user WHERE user_login = #{user_login}
    </select>
</mapper>